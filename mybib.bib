@article{liluqiJiYuShenDuXueXiHeBianYuanJianCeDeDongTaiChangJingXiaLuBangSLAM2021,
  title = {{{基于深度学习和边缘检测的动态场景下鲁棒SLAM}}},
  author = {李璐琪 and 蔡成林},
  year = {2021},
  journal = {传感技术学报},
  volume = {34},
  number = {01},
  pages = {80--88},
  abstract = {机器人在执行同时定位与地图创建(simultaneous localization and mapping, SLAM)的复杂任务时,容易受到移动物体的干扰,导致定位精度下降、地图可读性较差、系统鲁棒性不足,为此提出一种基于深度学习和边缘检测的SLAM算法。首先,利用YOLOv4目标检测算法获取场景中的语义信息,得到初步的语义动静态区域,同时提取ORB特征点并计算光流场,筛选动态特征点,通过语义关联进一步得到动态物体,利用canny算子计算动态物体的轮廓边缘,利用动态物体以外的静态特征点进行相机位姿估计,筛选关键帧,进行点云叠加,利用剔除动态物体的点云信息构建静态环境地图。本文算法在公开数据集上与ORB\_SLAM2进行对比,定位精度提升90\%以上,地图可读性明显增强,实验结果表明本文算法可以有效降低移动物体对定位与建图的影响,显著提升算法稳健性。},
  isbn = {1004-1699},
  keywords = {同时定位与地图创建;深度学习;目标检测;语义信息;动态场景;边缘检测}
}
